{
  "name": "proposify-design-system",
  "version": "0.1.6",
  "private": false,
  "homepage": "http://aguiar-tiago.github.io/proposify-design-system",
  "main": "dist/index.js",
  "author": "Proposify Inc.",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "@styled-system/should-forward-prop": "^5.1.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "actions": "^1.3.0",
    "axios": "^0.19.2",
    "emotion-theming": "^10.0.27",
    "lodash": "^4.17.20",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.4.2",
    "react-spinners-kit": "^1.9.1",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "babel-src": "babel src --out-dir dist --ignore \"**/__tests__/*.test.js\",\"**/*.stories.js\",\"**/testUtils/**/*.js\" -s",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "build": "run-s build:index clean babel-src",
    "build:index": "node ./config/buildIndex.js",
    "prepublish": "rm -rf dist && NODE_ENV=production babel src/components/* --out-dir dist --copy-files",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest \"src/.*\\.test\\.js\"",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public --no-manager-cache",
    "build-storybook": "build-storybook -s public",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.7.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@svgr/cli": "^5.5.0",
    "@testing-library/react": "^8.0.9",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "camelcase": "^5.0.0",
    "capitalize": "^1.0.0",
    "core-js": "^3.4.1",
    "cross-env": "^6.0.3",
    "full-icu": "^1.3.0",
    "gh-pages": "^3.1.0",
    "jest": "^24.9.0",
    "jest-dom": "^3.5.0",
    "jsdom": "^12.0.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-docgen": "^5.3.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.0",
    "webpack": "4.42.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.2"
  }
}
